create_table "users", force: :cascade do |t|
  t.string "name"
  t.integer "age"
  t.string "email"
  t.string "icon"
  t.string "comment"
  t.integer "is_public"
  t.integer "is_accept_message"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
  t.index ["member_id"], name: "index_member_id_on_users"
end

create_table "member", force: :cascade do |t|
  t.integer "team_id"
  t.integer "user_id"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
end

create_table "message", force: :cascade do |t|
  t.integer "from_user_id"
  t.integer "to_user_id"
  t.string "comment"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
end

create_table "memo", force: :cascade do |t|
  t.integer "is_team"
  t.integer "team_id"
  t.integer "user_id"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
end

create_table "team", force: :cascade do |t|
  t.string "name"
  t.string "icon"
  t.integer "is_public"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
end

create_table "team_project", force: :cascade do |t|
  t.string "team_id"
  t.string "project_id"
  t.string "icon"
  t.integer "is_public"
  t.string "comment"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
end

create_table "project", force: :cascade do |t|
  t.string "name"
  t.string "icon"
  t.string "tag"
  t.integer "is_public"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
end

create_table "release", force: :cascade do |t|
  t.integer "project_id"
  t.string "name"
  t.string "icon"
  t.string "tag"
  t.integer "is_public"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
end

create_table "review", force: :cascade do |t|
  t.integer "release_id"
  t.integer "star"
  t.string "comment"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
end

create_table "is_open", force: :cascade do |t|
  t.integer "user_id"
  t.integer "age"
  t.integer "join_team"
  t.integer "join_project"
  t.integer "comment"
  t.datetime "created_at", null: false
  t.timestamp "updated_at", null: false
end
